{
    "name": "phpbb/phpbb-website",
    "description": "The phpBB website located at www.phpbb.com",
    "version": "4.0.0-dev",
    "keywords": ["website", "phpbb"],
    "homepage": "http://www.phpbb.com",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "phpBB Group",
            "email": "operations@phpbb.com",
            "homepage": "http://phpbb.com/"
        }
    ],
    "support": {
        "irc": "irc://irc.freenode.org/phpbb-dev",
        "issues": "https://www.phpbb.com/bugs/website/"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "doctrine/common": "2.3.*",
        "doctrine/dbal": "2.3.*",
        "doctrine/orm": "2.3.*",
        "doctrine/doctrine-bundle": "1.0.*",
        "doctrine/doctrine-migrations-bundle": "dev-master@dev",
        "doctrine/migrations": "dev-master@dev",
        "jms/di-extra-bundle": "1.1.*",
        "jms/security-extra-bundle": "1.2.*",
        "kriswallsmith/assetic": "1.1.*@dev",
        "php": ">=5.3.3",
        "sensio/distribution-bundle": "2.1.*",
        "sensio/framework-extra-bundle": "2.1.*",
        "sensio/generator-bundle": "2.1.*",
        "symfony/assetic-bundle": "2.1.0",
        "symfony/monolog-bundle": "2.1.*",
        "symfony/swiftmailer-bundle": "2.1.*",
        "symfony/symfony": "2.1.*",
        "twig/extensions": "1.0.*@dev",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/data-fixtures": "dev-master",
        "twig/twig": "1.10.*",
        "phpbb/static-pages-bundle": "dev-master",
        "phpbb/update-git-repo-bundle": "dev-master",
        "phpbb/phpbb-sessions-auth-bundle": "dev-master",
        "phpbb/logging-admin-interface-bundle": "dev-master",
        "phpbb/dev-tools-bundle": "dev-master",
        "friendsofsymfony/user-bundle": "dev-master@dev",
        "sonata-project/admin-bundle": "dev-master@dev",
        "sonata-project/doctrine-orm-admin-bundle": "dev-master@dev",
        "sonata-project/jquery-bundle": "dev-master@dev",
        "sonata-project/exporter": "dev-master@dev",
        "sonata-project/block-bundle": "dev-master@dev",
        "sonata-project/cache-bundle": "dev-master@dev",
        "sonata-project/user-bundle": "dev-master@dev",
        "sonata-project/doctrine-extensions": "dev-master@dev",
        "sonata-project/easy-extends-bundle": "dev-master@dev",
        "sonata-project/google-authenticator": "dev-master@dev"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "beta",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
